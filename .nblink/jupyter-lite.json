{
  "jupyter-config-data": {
    "enableMemoryStorage": true,
    "contentsStorageDrivers": ["memoryStorageDriver"],
    "spectaConfig": {
      "topBar": {
        "title": "JupyterGames"
      },
      "defaultLayout": "default",
      "perFileConfig": {
        "notebooks/blog.ipynb": {
          "hideTopbar": false,
          "hideHeader": false,
          "defaultLayout": "article",
          "topBar": {
            "title": "pyb2d3",
            "themeToggle": false
          },
          "executionDelay": 3000
        }
      },
      "labConfig": {
        "setSingleMode": true,
        "hideLeftPanel": true,
        "hideRightPanel": true,
        "hideStatusbar": true,
        "hideHeader": false,
        "executionDelay": 3000
      }
    },
    "disabledExtensions": [
      "@jupyterlab/notebook-extension:open-with-no-kernel",
      "@jupyterlite/application-extension:opener"
    ]
  }
}
